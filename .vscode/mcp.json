{
  "inputs": [],
  "servers": {
    "Bicep Generator": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/src/BicepGeneratorMcp"
      ],
      "env": {
        "AZURE_OPENAI_ENDPOINT": "https://mcp-ai-test.openai.azure.com/"
      }
    },
    "Bicep": {
      "type": "stdio",
      "command": "dnx",
      "args": [
        "--source",
        "${workspaceFolder}/packages",
        "Bicep.McpServer@0.39.290-gd4b28b6ef1"
      ]
    }
  }
}